<?xml version="1.0" encoding="ISO-8859-1" ?>
<jsp:root version="1.2" xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ice="http://www.icesoft.com/icefaces/component">
	<jsp:directive.page contentType="text/html;charset=utf-8"
		pageEncoding="ISO-8859-1" />
	<f:view>
		<html>
			<head>
				<title>Software Administracion de Planchas e inHabilidades-
					Administracion de Inhabilidades - Recibir Plancha</title>

				<link rel="stylesheet" type="text/css"
					href="../estilos/rime/rime-portlet.css" />
				<link rel="stylesheet" type="text/css"
					href="../estilos/estiloPlanchas.css" />
				<f:loadBundle basename="co.com.coomeva.ele.etiquetas.etiquetas"
					var="etiquetas" />
			</head>
			<body bgcolor="#DFDFDF">

				<ice:form partialSubmit="true">
					<div align="center">
						<table cellpadding="0" cellspacing="0" width="824">
							<tr>
								<td colspan="2">
									<div align="left" class="banner"></div>
								</td>
							</tr>
							<tr>
								<td width="130" align="left" valign="top" bgcolor="#FFFFFF">
									<br />
									<br />
									<jsp:directive.include file="../general/menu.jsp" />

								</td>

								<td align="center" style="background: white; width: 706px;"
									height="300" valign="top">
									<br />
									<br />
									<ice:panelCollapsible columns="1" id="pnlPrincipal"
										expanded="true" toggleOnClick="false" style="width: 300px"
										rendered="#{numerarPlanchasCCEE.renderCollapsible}">

										<f:facet name="header">
											<ice:outputText id="lblTituloInicioSesion"
												style="font-size: 13px" value="#{etiquetas.ttlBuscar}"
												styleClass="">
											</ice:outputText>
										</f:facet>

										<br />

										<div align="center">
											<ice:panelGrid columns="2">
												<ice:outputText id="msgLogin"
													value="#{etiquetas.lblZonaElectoral}"
													styleClass="textoLabel" />
												<ice:selectOneMenu id="tipoTransaccion"
													binding="#{numerarPlanchasCCEE.zonasElectorales}">
													<f:selectItems
														value="#{numerarPlanchasCCEE.listZonasElectorales}" />
												</ice:selectOneMenu>
											</ice:panelGrid>
										</div>

										<br />
										<div align="center">
											<ice:commandButton id="btnBuscar" styleClass="button"
												value="#{etiquetas.lblBuscar}"
												action="#{numerarPlanchasCCEE.action_BuscarPlanchas}">
											</ice:commandButton>
										</div>
									</ice:panelCollapsible>
									<br />
									<br />


								</td>
							</tr>
						</table>


						<jsp:directive.include file="../popup/popupNumerarPlanchas.jsp" />
						
						<jsp:directive.include file="../popup/popupConfirmarBorrarNoPlancha.jsp" />
						
						<jsp:directive.include file="../popup/popupNumerarPlanchasOk.jsp" />

						<jsp:directive.include file="../popup/popupMensajeExcepcion.jsp" />
					</div>
				</ice:form>

			</body>
		</html>
	</f:view>
</jsp:root>

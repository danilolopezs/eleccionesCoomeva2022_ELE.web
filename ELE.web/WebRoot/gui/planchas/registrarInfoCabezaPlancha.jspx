													<ice:panelGrid columns="2">
														<ice:outputText
															value="#{etiquetas.lblCuentaPersonalCorreoElectronico}"
															styleClass="textoLabel" />
														<ice:panelGrid columns="2">
															<ice:inputText maxlength="35"
																value="#{registrarInfoCabezaPlancha.dtoInfo.cuentaPersonalMail}"

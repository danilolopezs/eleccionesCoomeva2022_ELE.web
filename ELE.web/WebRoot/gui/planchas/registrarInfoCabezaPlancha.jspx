													<ice:panelGrid columns="1">
														<ice:inputTextarea cols="106" rows="3"
															value="#{registrarInfoCabezaPlancha.dtoInfo.otrosEstudios}"
															styleClass="textoInput" maxlength="100" />
													</ice:panelGrid>
												</ice:panelGroup>
												<ice:outputText value="#{etiquetas.lblObligatoriedad}" />

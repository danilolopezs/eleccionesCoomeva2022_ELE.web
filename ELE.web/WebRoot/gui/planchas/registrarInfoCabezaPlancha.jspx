															<f:convertDateTime type="date" timeZone="GMT-5"
																pattern="yyyy/MM/dd" />
														</ice:selectInputDate>
													</ice:panelGrid>
												</ice:panelGroup>
												<ice:outputText value="#{etiquetas.lblObligatoriedad}" />

